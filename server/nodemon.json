{
	"restartable": "rs",
	"ignore": [".git", "node_modules/**/node_modules", "dist", "sessions"],
	"verbose": true,
	"execMap": {
		"js": "node"
	},
	"runOnChangeOnly": false,
	"watch": ["src/**/*.ts", "src/**/*.js"],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "ts js json",
	"exec": "ts-node  --transpile-only -r tsconfig-paths/register src/app.ts"
}
