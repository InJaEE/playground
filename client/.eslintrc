{
	"root": true,
	"env": {
		"node": true,
		"es2021": true
	},
	"parserOptions": {
		"parser": "babel-eslint",
		"sourceType": "module"
	},
	"extends": ["prettier", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"eqeqeq": "warn",
		"no-console": ["warn", { "allow": ["info", "error", "warn"] }],
		"camelcase": "off"
	}
}
